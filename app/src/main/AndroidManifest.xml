<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.hungnguyenbasv.d7_loginform">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/vietis_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.activity.LoginActivity">

            <!-- <activity android:name=".activity.activity.StartActivity"> -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.activity.MenuMainActivity" />
        <activity android:name=".activity.activity.RegisterActivity" />
        <activity android:name=".activity.activity.ForgotPasswordActivity" />
        <!-- <activity android:name=".activity.activity.LoginActivity" /> -->
        <activity android:name=".activity.activity.MenuActivity" />
        <activity android:name=".activity.activity.ProjectInforActivity"></activity>
    </application>

</manifest>